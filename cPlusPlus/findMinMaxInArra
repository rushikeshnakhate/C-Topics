int main()
{
	std::array< signed int,101 > arr{ 1, 2,3,4,4,5,6,6,77,87 , -1 , -10 };
	int max = *std::max_element(arr.begin(), arr.end());
	std::cout << max << std::endl; 


	int min = *std::min_element(arr.begin(), arr.end());
	std::cout << min << std::endl;

	//sum_1(1,23,3,4,5);
	//using c++ folds 


}
